<% if @feed_items.any? %>
  <ol class="microposts">
  <!-- Rails knows to call the micropost partial because each element of -->
  <!-- @feed_items has class Micropost. This causes Rails to look for a partial -->
  <!-- with the corresponding name in the views -->
    <%= render @feed_items %>
  </ol>
  <%= will_paginate @feed_items,
                    params: { controller: :static_pages, action: :home }%>
<% end %>